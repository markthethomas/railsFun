<p id="notice"><%= notice %></p>

<h1>Listing Pins</h1>

<table>
  <thead>
    <tr>
      <th>Url</th>
      <th>Title</th>
      <th>Crawled</th>
      <th>Imageurl</th>
      <th>Remoteimageurl</th>
      <th>Createdat</th>
      <th>Updatedat</th>
      <th>Createdbyuser</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pins.each do |pin| %>
      <tr>
        <td><%= pin.url %></td>
        <td><%= pin.title %></td>
        <td><%= pin.crawled %></td>
        <td><%= pin.imageUrl %></td>
        <td><%= pin.remoteImageUrl %></td>
        <td><%= pin.createdAt %></td>
        <td><%= pin.updatedAt %></td>
        <td><%= pin.createdByUser %></td>
        <td><%= link_to 'Show', pin %></td>
        <td><%= link_to 'Edit', edit_pin_path(pin) %></td>
        <td><%= link_to 'Destroy', pin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pin', new_pin_path %>
